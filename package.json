{
  "name": "remote-design-tokens",
  "author": "C. Lewis <ctj.lewis@icloud.com>",
  "type": "module",
  "version": "0.0.5",
  "license": "MIT",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "preboot": "yarn clean",
    "boot": "tsc && rollup -c --silent",
    "bootstrap": "yarn boot && yarn build",
    "dev": "tszip dev",
    "build": "tszip build",
    "test": "dotenv -e .env.local tszip test",
    "test:watch": "tszip test --watch",
    "test:coverage": "tszip test --coverage",
    "lint": "tszip lint",
    "prepare": "tszip build"
  },
  "module": "./dist/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./*": "./dist/*/index.js"
  },
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "devDependencies": {
    "@tszip/tszip": "^1.15.2",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "husky": "^7.0.4",
    "isomorphic-fetch": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "yarn-deduplicate": "^3.1.0"
  },
  "dependencies": {
    "dotenv-cli": "^4.1.0"
  }
}
